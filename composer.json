{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.0",
        "ext-pdo": "*",
        "bensampo/laravel-enum": "^4.1",
        "classic-o/nova-media-library": "^1.0",
        "customs/columns-card": "*",
        "darkaonline/l5-swagger": "8.0.9",
        "digital-creative/collapsible-resource-manager": "^1.4",
        "doctrine/dbal": "^2.5",
        "dyrynda/laravel-cascade-soft-deletes": "^4.1",
        "fakerphp/faker": "^1.9.1",
        "fideloper/proxy": "^4.4",
        "fruitcake/laravel-cors": "^2.0",
        "guzzlehttp/guzzle": "^6.0",
        "inspheric/nova-indicator-field": "^1.43",
        "jagdeepbanga/nova-date-time": "^1.0",
        "laravel/framework": "^8.65",
        "laravel/nova": "*",
        "laravel/sanctum": "^2.11",
        "laravel/scout": "^9.4",
        "laravel/tinker": "^2.5",
        "league/flysystem-aws-s3-v3": "~1.0",
        "nao-pon/flysystem-google-drive": "1.1.0",
        "spatie/laravel-permission": "^5.4",
        "spatie/laravel-query-builder": "^4.0",
        "spatie/nova-backup-tool": "^4.2",
        "symfony/console": "^5.2",
        "symfony/http-client": "^6.0",
        "symfony/validator": "^5.2"
    },
    "require-dev": {
        "facade/ignition": "^2.5",
        "kitloong/laravel-migrations-generator": "^5.1",
        "laravel/sail": "^1.0.1",
        "mockery/mockery": "^1.4.4",
        "nunomaduro/collision": "^5.10",
        "orangehill/iseed": "^3.0",
        "php-parallel-lint/php-parallel-lint": "^1.3",
        "phpmd/phpmd": "^2.11",
        "phpro/grumphp": "^1.7",
        "phpstan/phpstan": "^1.4",
        "squizlabs/php_codesniffer": "^3.6",
        "phpunit/phpunit": "^9.5.10"
    },
    "autoload": {
        "files": [
            "app/helpers.php"
        ],
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "repositories": [
        {
            "type": "path",
            "url": "./nova"
        },
        {
            "type": "path",
            "url": "./nova-components/ColumnsCard"
        }
    ],
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "phpro/grumphp": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
