{
    "name": "customs/columns-card",
    "description": "A Laravel Nova card for selecting resource columns on index.",
    "keywords": [
        "laravel",
        "nova"
    ],
    "license": "MIT",
    "require": {
        "php": ">=7.1.0"
    },
    "autoload": {
        "psr-4": {
            "Customs\\ColumnsCard\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Customs\\ColumnsCard\\CardServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
