{
  "include": [
    "resources/js/**/*",
    "resources/js/**/*.vue"
  ],
  "compilerOptions": {
    "target": "ESNext",
    "module": "ESNext",
    "moduleResolution": "Node",
    "strict": true,
    "resolveJsonModule": true,
    "isolatedModules": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "allowJs": true,
    "baseUrl": "./resources/js",
    "paths": {
      "@/*": ["./*"],
    }
  }
}
